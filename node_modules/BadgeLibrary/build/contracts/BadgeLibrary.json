  "source": "pragma solidity ^0.4.15;\n\nlibrary BadgeLibrary {\n\n    /// @notice represents simple details about a earnable badge\n    struct Badge {\n        address issuer;\n        string description;\n        string name;\n        string image;\n        string version; \n        // bytes32 linkHash;\n        // bytes32 chainHash;\n    }    \n\n    /// @notice represents details of an issued badge \n    struct Credential {\n        Badge badge;\n        uint expires;\n        address recipient;\n        bytes32 txKey;\n    }\n    /// @notice generate a credential transaction key\n    function credentialTxKey(address _issuer, bytes _msgData, uint _nonce) constant public returns(bytes32 key) {\n        return keccak256(_issuer, _msgData, _nonce);\n    }\n    /// @notice compute hash of badgename \n    function getBadgeNameHash(string _badgename) constant public returns (bytes32 _hash) {\n        return keccak256(_badgename);\n    }\n\n    /// @notice compute hash for badge linking\n    function getBadgeLinkHash(string _name, address _issuer) constant public returns (bytes32 hash) {\n        return keccak256(_name, _issuer);\n    }\n}",
