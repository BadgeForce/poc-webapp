{
    "extends": ["eslint:recommended", "google"],
    "parser": "typescript-eslint-parser",
    "plugins": [
        "typescript"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules":{
        "require-jsdoc": [0],
        "max-len": [0],
        "no-tabs": [0],
        "no-console": [1],
        "camelcase": [1],
        "no-undef": [1],
        "no-unused-vars": [1],
        "no-mixed-spaces-and-tabs": [1],
        "valid-jsdoc": [1]
    }
}
